@layout("/common/_form.html",{js:["/assets/cityFile/cityFile_add.js"],css:["/assets/expand/module/formSelects/formSelects-v4.css"]}){

<form class="layui-form" id="cityFileForm" lay-filter="cityFileForm">
    <div class="layui-fluid" style="padding-bottom: 75px;">
        <div class="layui-card">
            <div class="layui-card-header">基本信息</div>
            <div class="layui-card-body">
                <div class="layui-form-item layui-row">
                    <input name="id" type="hidden"/>

                    <div class="layui-inline layui-col-md12">
                        <div class="layui-input-block">
                            <input id="FILENAME" name="FILENAME" placeholder="请输入" type="text" class="layui-input" lay-verify="required" required/>
                        </div>
                        <button class="layui-form-label">选择文件<span style="color: red;">*</span></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="form-group-bottom text-center">
        <button class="layui-btn" lay-filter="btnSubmit" lay-submit>&emsp;提交&emsp;</button>
        <button type="reset" class="layui-btn layui-btn-primary" ew-event="closeDialog">&emsp;取消&emsp;</button>
    </div>

</form>

@}